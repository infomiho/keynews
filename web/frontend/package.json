{
  "private": true,
  "name": "frontend2",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run -s serve || npm run -s dev",
    "build": "preact build --dest ../app/app/static --clean --no-prerender",
    "serve": "preact build && preact serve",
    "dev": "preact watch",
    "lint": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint-config-synacor"
  },
  "eslintIgnore": [
    "build/*"
  ],
  "devDependencies": {
    "eslint": "^4.5.0",
    "eslint-config-synacor": "^1.1.0",
    "if-env": "^1.0.0",
    "node-sass": "^4.6.1",
    "preact-cli": "^3.4.3",
    "sass-loader": "^13.2.0"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "bulma": "^0.6.1",
    "cytoscape": "^3.2.5",
    "cytoscape-cose-bilkent": "^4.0.0",
    "cytoscape-spread": "^1.3.1",
    "moment": "^2.20.1",
    "preact": "^8.2.1",
    "preact-compat": "^3.17.0",
    "preact-redux": "^2.0.3",
    "preact-router": "^2.5.5",
    "randomcolor": "^0.5.3",
    "react-autosuggest": "^9.3.2",
    "react-cytoscape": "^1.0.5",
    "react-datepicker": "^1.0.4",
    "redux": "^3.7.2",
    "spinkit": "^1.2.5"
  }
}
